<nav class="my-5" aria-label="Page navigation example">
  <ul class="pagination justify-content-center">
    <li class="page-item <% if(page.currentPage==1) { %> disabled <% } %>">
      <a class="page-link" href="/?page=<%- page.currentPage-1 %>" tabindex="-1">Previous</a>
    </li>
    <% for(var i=1; i<=page.pageTotal; i++){ %>
      <li class="page-item <% if(i==page.currentPage) { %> active <% } %>"> 
        <a class="page-link" href="/?page=<%- i %>">
         <%- i %>
        </a>
      </li>
    <% } %>
    
    
    <li class="page-item <% if(page.currentPage==page.pageTotal) { %> disabled <% } %>">
      <a class="page-link" href="/?page=<%- page.currentPage+1 %>">Next</a>
    </li>
  </ul>
</nav>